            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="evilmasterbase::Salle" referenceName="salle">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'Salle '+container.salle->size()"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="price" valueExpression="20"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="description" valueExpression="Une tres belle salle"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="CC_Couloir" label="New Couloir" containerMappings="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='D_Repere']/@defaultLayer/@containerMappings[name='C_Salle']">
