            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql: source.eContainer()">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="evilmasterbase::Porte" referenceName="porte">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="salle1" valueExpression="aql:source"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="salle2" valueExpression="aql:target"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="price" valueExpression="1"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql: 'Porte ' + source.eContainer().porte->size()"/>
